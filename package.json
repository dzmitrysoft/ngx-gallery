{
  "name": "ngx-gallery",
  "version": "10.0.0",
  "description": "Angular gallery directive that hooks the lightbox with the images automatically.",
  "homepage": "http://github.com/murhafsousli/ngx-gallery",
  "author": {
    "name": "Murhaf Sousli",
    "url": "http://github.com/murhafsousli"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/murhafsousli/ngx-gallery.git"
  },
  "bugs": {
    "url": "http://github.com/murhafsousli/ngx-gallery/issues"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start-demo": "ng serve demo",
    "start-demo-aot": "ng serve demo --aot",
    "build-demo": "ng build demo --prod --base-href https://murhafsousli.github.io/ngx-gallery/",
    "init-build": "npm run build-core && npm run version && npm run link-core && npm run build-lightbox && npm run version && npm run link-lightbox && npm run build-gallerize && npm run version && npm run link-gallerize && npm run version",
    "build-all": "ng build core --prod && ng build lightbox --prod && ng build gallerize --prod && npm run version",
    "build-core": "ng build core",
    "build-lightbox": "ng build lightbox",
    "build-gallerize": "ng build gallerize",
    "link-core": "npm link dist/core && npm link @ngx-gallery/core",
    "link-lightbox": "npm link dist/lightbox && npm link @ngx-gallery/lightbox",
    "link-gallerize": "npm link dist/gallerize && npm link @ngx-gallery/gallerize",
    "publish-all": "node .config/publish.js",
    "version": "node .config/version.js",
    "build": "ng build --prod",
    "npm_pack": "npm pack",
    "package": "npm run build && npm run npm_pack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.4",
    "@angular/cdk": "^10.0.2",
    "@angular/common": "^10.0.4",
    "@angular/compiler": "^10.0.4",
    "@angular/core": "^10.0.4",
    "@angular/forms": "^10.0.4",
    "@angular/material": "^10.0.2",
    "@angular/platform-browser": "^10.0.4",
    "@angular/platform-browser-dynamic": "^10.0.4",
    "@angular/router": "^10.0.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@ngx-progressbar/core": "^5.3.2",
    "@ngx-progressbar/http": "^5.3.2",
    "@ngx-progressbar/router": "^5.3.2",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.6.5",
    "hammerjs": "^2.0.8",
    "highlight.js": "^10.1.1",
    "ngx-highlightjs": "^4.1.0-beta",
    "ngx-scrollbar": "^7.2.3",
    "rxjs": "^6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.3",
    "@angular-devkit/build-ng-packagr": "~0.1000.3",
    "@angular/cli": "^10.0.3",
    "@angular/compiler-cli": "^10.0.4",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/language-service": "^10.0.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "ansi-colors": "^3.0.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "codelyzer": "^6.0.0",
    "protractor": "~7.0.0",
    "replace-ext": "^1.0.0",
    "ts-node": "~5.0.1",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7"
  }
}
